{
  "private": true,
  "scripts": {
    "build": "spago build",
    "build:examples": "spago -x dev.dhall build",
    "watch:examples": "spago -x dev.dhall watch",
    "repl": "spago -x dev.dhall repl",
    "format": "purs-tidy format-in-place src examples",
    "serve-example": "npm run build:examples && parcel serve --open --dist-dir dev examples/Example/$npm_config_module/index.html",
    "example:Run": "npm run serve-example --module=Run",
    "example:TransformPath": "npm run serve-example --module=TransformPath",
    "example:Pattern": "npm run serve-example --module=Pattern"
  },
  "devDependencies": {
    "bower": "^1.8.13",
    "concurrently": "^7.0.0",
    "npm-upgrade": "^3.1.0",
    "parcel": "^2.3.1",
    "pscid": "^2.9.3",
    "pulp": "^15.0.0",
    "purescript": "^0.14.5",
    "purs-tidy": "^0.7.0",
    "spago": "^0.20.7"
  }
}
